<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
   <SelectDialog
           title="Choose From List"
           class="sapUiPopupWithPadding"
           items="{cfl>/}"
           search="_onSearch"
           confirm="_onConfirm"
           cancel="_onClose">
      <StandardListItem
              title="{cfl>id}"
              description="{cfl>bpCode}"
              type="Active" />
   </SelectDialog>
</core:FragmentDefinition>